<?xml version="1.0" encoding="UTF-8" ?>
<Page loaded="pageLoaded">
  <ScrollView>
    <Repeater items="{{ items }}">
      <Repeater.itemsLayout>
        <WrapLayout />
      </Repeater.itemsLayout>
      <Repeater.itemTemplate>
        <Label text="{{ title }}" margin="10" />
      </Repeater.itemTemplate>
    </Repeater>
  </ScrollView>
</Page>