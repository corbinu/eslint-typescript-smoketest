{
  "name": "eslint-typescript-smoketest",
  "version": "0.1.0",
  "description": "Smoke tests for eslint-typescript-parser using code from large typescript projects",
  "main": "index.js",
  "devDependencies": {
    "copy": "^0.3.0",
    "eslint": "4.0.0-alpha.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash": "^2.3.5",
    "eslint-plugin-mysticatea": "^4.1.0",
    "eslint-plugin-node": "^4.0.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-typescript": "git+https://github.com/corbinu/eslint-plugin-typescript.git#smoke",
    "eslint-plugin-unicorn": "^2.0.1",
    "fs-extra": "^3.0.0",
    "typescript": "^2.2.0",
    "typescript-eslint-parser": "git+https://github.com/eslint/typescript-eslint-parser.git"
  },
  "scripts": {
    "copy": "node bin/copy.js",
    "vscode": "eslint \"src/vscode/**/*.ts\"",
    "vscode-all": "eslint --config .all.json \"src/vscode/src/vs/base/browser/ui/list/listWidget.ts\"",
    "vscode-plugin": "eslint --config .plugins.json \"src/vscode/**/*.ts\"",
    "vscode.d": "eslint \"src/vscode/**/*.d.ts\"",
    "vscode.d-all": "eslint --config .all.json \"src/vscode/**/*.d.ts\"",
    "vscode.d-plugin": "eslint --config .plugins.json \"src/vscode/**/*.d.ts\"",
    "groceries": "eslint \"src/nativescript-sample-groceries/**/*.ts\"",
    "groceries-all": "eslint --config .all.json \"src/nativescript-sample-groceries/**/*.ts\"",
    "groceries-plugin": "eslint --config .plugins.json \"src/nativescript-sample-groceries/**/*.ts\"",
    "groceries.d": "eslint \"src/nativescript-sample-groceries/**/*.d.ts\"",
    "glimmer": "eslint \"src/glimmer/**/*.ts\"",
    "glimmer-all": "eslint --config .all.json \"src/glimmer/**/*.ts\"",
    "glimmer-plugin": "eslint --config .plugins.json \"src/glimmer/**/*.ts\"",
    "glimmer.d": "eslint \"src/glimmer.d/**/*.d.ts\"",
    "nativescript": "eslint \"src/nativescript/**/*.ts\"",
    "nativescript-all": "eslint --config .all.json \"src/nativescript/**/*.ts\"",
    "nativescript-plugin": "eslint --config .plugins.json \"src/nativescript/**/*.ts\"",
    "nativescript.d": "eslint \"src/nativescript/**/*.d.ts\"",
    "angular": "eslint \"src/angular/**/*.ts\"",
    "angular-all": "eslint --config .all.json \"src/angular/**/*.ts\"",
    "angular-plugin": "eslint --config .plugins.json \"src/angular/**/*.ts\"",
    "angular.d": "eslint \"src/angular/**/*.d.ts\"",
    "typings": "eslint \"src/typings/**/*.d.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/corbinu/eslint-typescript-parser.git"
  },
  "keywords": [
    "smoketest",
    "eslint",
    "typescript"
  ],
  "author": "Corbin Uselton <c@decimal.io>",
  "engines": {
    "npm": "^2.0.0",
    "node": ">=4.2.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/corbinu/eslint-typescript-parser/issues"
  },
  "homepage": "https://gitlab.com/corbinu/eslint-typescript-parser#README",
  "dependencies": {
    "eslint-plugin-typescript": "git+https://github.com/corbinu/eslint-plugin-typescript.git#smoke",
    "shelljs": "^0.7.6",
    "typescript-eslint-parser": "git+https://github.com/eslint/typescript-eslint-parser.git"
  }
}
