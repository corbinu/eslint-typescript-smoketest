{
	// See https://go.microsoft.com/fwlink/?LinkId=759670
	// for the documentation about the jsconfig.json format
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true
	},
	"exclude": [
		"src",
		"extensions",
		"test",
		"i18n",
		"out",
		"out-build",
		"out-vscode",
		"out-vscode-min",
		"out-editor",
		"out-editor-min",
		"out-monaco-editor-core",
		"resources",
		"scripts",
		".build",
		"node_modules",
		"build/monaco/node_modules"
	]
}
