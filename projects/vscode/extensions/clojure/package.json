{
	"name": "clojure",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"contributes": {
		"languages": [{
			"id": "clojure",
			"aliases": ["Clojure", "clojure"],
			"extensions": [".clj", ".cljs", ".cljx", ".clojure", ".edn"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "clojure",
			"scopeName": "source.clojure",
			"path": "./syntaxes/Clojure.tmLanguage"
		}]
	}
}